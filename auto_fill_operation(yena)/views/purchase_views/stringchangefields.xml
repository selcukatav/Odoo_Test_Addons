<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_order_form_inherit_string" model="ir.ui.view">
            <field name="name">purchase.order.form.inherit.string.yena</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="attributes">
                    <attribute name="string">Supplier Name</attribute>
                </xpath>
                <xpath expr="//field[@name='company_id']" position="attributes">
                    <attribute name="string">Company (Buyer)</attribute>
                </xpath>
                <xpath expr="//field[@name='x_project_purchase']" position="attributes">
                    <attribute name="string">Project Number</attribute>
                </xpath>
                <xpath expr="//field[@name='x_order_date']" position="attributes">
                    <attribute name="string">S-RFQ Date</attribute>
                </xpath>
                <xpath expr="//label[@for='date_planned']" position="attributes">
                    <attribute name="string">S-Required Delivery Date</attribute>
                </xpath>
                <xpath expr="//field[@name='date_order']" position="attributes">
                    <attribute name="string">Latest Bid Date</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
